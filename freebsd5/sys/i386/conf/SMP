# $FreeBSD: src/sys/i386/conf/SMP,v 1.4 2003/03/26 19:49:34 ps Exp $

include GENERIC

ident		SMP-GENERIC

# To make an SMP kernel, the next two are needed
options 	SMP			# Symmetric MultiProcessor Kernel
options 	APIC_IO			# Symmetric (APIC) I/O
