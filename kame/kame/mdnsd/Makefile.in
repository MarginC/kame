#	$KAME: Makefile.in,v 1.1 2000/05/21 03:18:53 itojun Exp $

srcdir=	@srcdir@
CFLAGS=	@CFLAGS@ @DEFS@
LDFLAGS=@LDFLAGS@
LIBOBJS=@LIBOBJS@
LIBS=	@LIBS@
CC=	@CC@
TARGET=	mdnsd

INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_DATA=@INSTALL_DATA@
prefix=	@prefix@
exec_prefix=	@exec_prefix@
bindir=	@bindir@
sbindir=@sbindir@
mandir=	@mandir@

OBJS=	mdnsd.o mainloop.o

all:	$(TARGET) 
mdnsd:	$(OBJS) $(LIBOBJS)
	$(CC) $(LDFLAGS) -o mdnsd $(OBJS) $(LIBOBJS) $(LIBS) 

install::

includes::

clean::
	/bin/rm -f *.o $(TARGET)

distclean:: clean
	/bin/rm -f Makefile config.cache config.log config.status .depend

depend:
	mkdep ${CFLAGS:M-[ID]*} $(srcdir)/*.c
